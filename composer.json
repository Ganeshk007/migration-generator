{
    "name": "ganeshk007/migration-generator",
    "description": "A Laravel package for generate migrations",
    "type": "library",
    "keywords": [
        "laravel",
        "migrations",
        "generate migrations"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ganesh",
            "email": "ganesh100295@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ganesh\\MigrationGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ganesh\\MigrationGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ganesh\\MigrationGenerator\\MigrationGeneratorServiceProvider"
            ],
            "aliases": {
                "MigrationGenerator": "Ganesh\\MigrationGenerator\\Facades\\MigrationGenerator"
            }
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
